{
  "name": "hub",
  "version": "1.0.0",
  "description": "a hub for studying node",
  "main": "./src/main.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/main.ts",
    "clean": "del-cli ./dist/**",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "start": "node .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "nodemon": "nodemon  -e  ts,tsx  --exec ts-node ./src/main.ts"
  },
  "keywords": [
    "hub"
  ],
  "author": "tutu",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "cos-nodejs-sdk-v5": "^2.12.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jsonwebtoken": "^9.0.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-multer": "^1.0.2",
    "koa-router": "^12.0.0",
    "mysql2": "^3.5.2",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/koa-router": "^7.4.4",
    "@types/koa": "^2.13.6",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^6.1.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "eslint --fix --ignore-path .eslintignore",
    "src/**/*.{less,sass,css,vue}": "stylelint --fix --ignore-path .eslintignore"
  }
}
